{
  "manifest_version": 3,
  "name": "Fruit Timer",
  "version": "1.0",
  "description": "Helpt je te focussen en dingen voor elkaar te krijgen",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Fruit Timer",
    "default_popup": "popup/popup.html" 
  },
  "permissions": ["storage", "alarms", "notifications"],
  "background": {
    "service_worker": "background.js"
},
"options_page": "options/options.html"
}